   "source": [
    "# Dividing the duration column into 3 different groups (0-30, 31-60, 60 and above)\n",
    "bins = [0, 30, 60, float('inf')]\n",
    "labels = [0, 1, 2]\n",
    "df['duration '] = pd.cut(df['duration '], bins=bins, labels=labels, right=False).astype(int)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "state_mapping = {'failed': 0, 'canceled': 0, 'suspended':0, 'successful': 1}\n",
    "df['state '] = df['state '].map(state_mapping)"
   ]
  },
  {
   "cell_type": "code",
